{
  "name": "db-migration-framework",
  "version": "1.0.0",
  "description": "Enterprise-grade database migration framework supporting multiple database types",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "migrate": "node dist/cli/cli.js",
    "dashboard": "node dist/dashboard/server.js",
    "migrate:list": "node dist/cli/cli.js list-tables",
    "migrate:validate": "node dist/cli/cli.js validate",
    "migrate:all": "node dist/cli/cli.js migrate-all",
    "migrate:table": "node dist/cli/cli.js migrate-table",
    "migrate:collections": "node dist/cli/cli.js migrate-table --table COLLECTIONS",
    "migrate:modules": "node dist/cli/cli.js migrate-table --table MODULES",
    "migrate:roles": "node dist/cli/cli.js migrate-table --table ROLES",
    "migrate:category": "node dist/cli/cli.js migrate-table --table CATEGORY",
    "migrate:camera": "node dist/cli/cli.js migrate-table --table CAMERA",
    "migrate:camera-collections": "node dist/cli/cli.js migrate-table --table CAMERA_COLLECTION_MAP",
    "migrate:camera-modules": "node dist/cli/cli.js migrate-table --table CAMERA_MODULE_MAP"
  },
  "keywords": [
    "database",
    "migration",
    "vertica",
    "postgresql",
    "mssql",
    "data-transfer",
    "schema-migration",
    "enterprise"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "async": "^3.2.4",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "inquirer": "^9.2.12",
    "joi": "^17.11.0",
    "lodash": "^4.17.21",
    "mssql": "^10.0.1",
    "ora": "^7.0.1",
    "pg": "^8.16.3",
    "vertica": "^0.5.5",
    "winston": "^3.11.0",
    "ws": "^8.14.2",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/async": "^3.2.24",
    "@types/express": "^4.17.20",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.6",
    "@types/lodash": "^4.14.200",
    "@types/mssql": "^9.1.4",
    "@types/node": "^20.8.0",
    "@types/pg": "^8.15.5",
    "@types/ws": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
